{
  "name": "@storybook/root",
  "version": "8.5.0-beta.8",
  "private": true,
  "description": "Storybook root",
  "homepage": "https://storybook.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/storybook.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  },
  "type": "module",
  "workspaces": {
    "packages": [
      "./core",
      "./addons/*",
      "./frameworks/*",
      "./lib/*",
      "./deprecated/*",
      "./builders/*",
      "./presets/*",
      "./renderers/*"
    ]
  },
  "scripts": {
    "affected:test": "nx show projects --affected -t test | xargs -I # echo --project # | xargs yarn test:watch",
    "await-serve-storybooks": "wait-on http://localhost:8001",
    "bench-packages": "jiti ./scripts/bench/bench-packages.ts",
    "build": "yarn build-package",
    "build-package": "jiti ./scripts/build-package.ts",
    "changelog": "pr-log --sloppy --cherry-pick",
    "changelog:next": "pr-log --sloppy --since-prerelease",
    "check": "jiti ./scripts/prepare/check-scripts.ts",
    "check-package": "jiti ./scripts/heck-package.ts",
    "ci-tests": "yarn task --task check --no-link --start-from=install && yarn lint && yarn test && cd ../scripts && yarn test",
    "danger": "danger",
    "docs:prettier:check": "cd ./docs && prettier --check ./_snippets || echo 'Please run \"docs:prettier:write\" in the \"scripts\" directory to fix the issues'",
    "docs:prettier:write": "cd ./docs && prettier --write ./_snippets",
    "generate-sandboxes": "jiti ./scripts/sandbox/generate.ts",
    "get-report-message": "jiti ./scripts/get-report-message.ts",
    "get-template": "jiti ./scripts/get-template.ts",
    "github-release": "github-release-from-changelog",
    "i": "yarn --cwd .. i",
    "knip": "VITE_CJS_IGNORE_WARNING=1 knip --config ./scripts/knip.config.ts",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:ejs": "ejslint **/*.ejs",
    "lint:js": "yarn lint:js:cmd .  --quiet",
    "lint:js:cmd": "cross-env NODE_ENV=production eslint --cache --cache-location=./.cache/eslint --ext .js,.jsx,.json,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "lint:md": "remark -q .",
    "lint:other": "prettier --write '**/*.{css,html,json,md,yml}'",
    "lint:package": "sort-package-json",
    "local-registry": "jiti ./scripts/run-registry.ts",
    "publish": "jiti ./scripts/sandbox/publish.ts",
    "publish-sandboxes": "yarn publish",
    "release:cancel-preparation-runs": "jiti ./scripts/release/cancel-preparation-runs.ts",
    "release:generate-pr-description": "jiti ./scripts/release/generate-pr-description.ts",
    "release:get-changelog-from-file": "jiti ./scripts/release/get-changelog-from-file.ts",
    "release:get-current-version": "jiti ./scripts/release/get-current-version.ts",
    "release:get-version-changelog": "jiti ./scripts/release/get-version-changelog.ts",
    "release:is-pr-frozen": "jiti ./scripts/release/is-pr-frozen.ts",
    "release:is-prerelease": "jiti ./scripts/release/is-prerelease.ts",
    "release:is-version-published": "jiti ./scripts/release/is-version-published.ts",
    "release:label-patches": "jiti ./scripts/release/label-patches.ts",
    "release:pick-patches": "jiti ./scripts/release/pick-patches.ts",
    "release:publish": "jiti ./scripts/release/publish.ts",
    "release:unreleased-changes-exists": "jiti ./scripts/release/unreleased-changes-exists.ts",
    "release:version": "jiti ./scripts/release/version.ts",
    "release:write-changelog": "jiti ./scripts/release/write-changelog.ts",
    "storybook:ui": "NODE_OPTIONS=\"--preserve-symlinks --preserve-symlinks-main\" ./lib/cli/bin/index.cjs dev --port 6006 --config-dir ./.storybook",
    "storybook:ui:build": "NODE_OPTIONS=\"--preserve-symlinks --preserve-symlinks-main\" ./lib/cli/bin/index.cjs build --config-dir ./.storybook --webpack-stats-json",
    "storybook:ui:chromatic": "chromatic --build-script-name storybook:ui:build --storybook-base-dir ./ --exit-zero-on-changes --exit-once-uploaded",
    "storybook:vitest": "yarn test:watch --project storybook-ui",
    "storybook:vitest:inspect": "INSPECT=true yarn test --project storybook-ui",
    "strict-ts": "jiti ./scripts/strict-ts.ts",
    "task": "jiti ./scripts/task.ts",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 vitest run",
    "test:watch": "NODE_OPTIONS=--max_old_space_size=4096 vitest watch",
    "upgrade": "jiti ./scripts/task.ts",
    "upload-bench": "jiti ./scripts/upload-bench.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "if [ -z \"$SKIP_STORYBOOK_GIT_HOOKS\" ]; then yarn lint-staged; fi"
    }
  },
  "lint-staged": {
    "*.{html,js,json,jsx,mjs,ts,tsx}": [
      "yarn lint:js:cmd --fix"
    ],
    "package.json": [
      "yarn lint:package"
    ],
    "*.ejs": [
      "ejslint"
    ],
    "*.{css,html,json,md,yml}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "resolutions": {
    "@playwright/test": "1.48.1",
    "@types/babel__traverse@npm:*": "patch:@types/babel__traverse@npm%3A7.20.6#~/.yarn/patches/@types-babel__traverse-npm-7.20.6-fac4243243.patch",
