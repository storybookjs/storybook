{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "nxCloudAccessToken": "NGVmYTkxMmItYzY3OS00MjkxLTk1ZDktZDFmYTFmNmVlNGY4fHJlYWQ=",
  "defaultBase": "next",
  "parallel": 8,
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": false
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "targetDefaults": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn prep",
        "args": "--reset"
      },
      "configurations": {
        "production": {
          "args": "--reset --optimized"
        }
      },
      "inputs": [
        "production",
        "^production"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{workspaceRoot}/code/bench/esbuild-metafiles/{projectName}"
      ],
      "cache": true
    },
    "check": {
      "configurations": {
        "production": {}
      },
      "inputs": ["default", "^production"],
      "dependsOn": ["build"],
      "cache": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [
      "{workspaceRoot}/code/tsconfig.json",
      "{workspaceRoot}/scripts/**/*",
      "{workspaceRoot}/.github/workflows/nx.yml"
    ],
    "production": [
      "default",
      "!{projectRoot}/src/**/**/*.{test,spec,stories}.?(c|m)[jt]s?(x)?(.snap)",
      "!{projectRoot}/vitest.config.[jt]s",
      "!{projectRoot}/README.md",
      "!{projectRoot}/.eslintrc.{json,js}",
      "!{projectRoot}/src/test-setup.[jt]s"
    ]
  },
  "nxCloudId": "691dbcdcb2822ca4fdc727c4"
}
