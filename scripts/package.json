{
  "name": "@storybook/scripts",
  "version": "7.0.0-alpha.16",
  "private": true,
  "type": "module",
  "nx": {
    "includedScripts": []
  },
  "scripts": {
    "bench-packages": "jiti ./bench/bench-packages.ts",
    "build-package": "jiti ./build-package.ts",
    "check": "jiti ./check/check-package.ts",
