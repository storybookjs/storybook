  "dependenciesMeta": {
    "ejs": {
      "built": false
    },
    "level": {
      "built": false
    },
    "node-uuid": {
      "built": false,
      "unplugged": false
    },
    "nodemon": {
      "built": false
    },
    "parcel": {
      "built": false
    },
    "preact": {
      "built": false
    },
    "yorkie": {
      "built": false
    }
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=22.0.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  },
  "pr-log": {
    "skipLabels": [
      "cleanup"
    ],
    "validLabels": [
      [
        "BREAKING CHANGE",
        "Breaking Changes"
      ],
      [
        "feature request",
        "Features"
      ],
      [
        "bug",
        "Bug Fixes"
      ],
      [
        "documentation",
        "Documentation"
      ],
      [
        "maintenance",
        "Maintenance"
      ],
      [
        "build",
        "Build"
      ],
      [
        "dependencies",
        "Dependency Upgrades"
      ]
    ]
  }
