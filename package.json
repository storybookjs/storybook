    "@storybook/codemod": "workspace:*",
    "@storybook/core-webpack": "workspace:*",
    "@storybook/csf-plugin": "workspace:*",
    "@storybook/ember": "workspace:*",
    "@storybook/global": "^5.0.0",
    "@storybook/html": "workspace:*",
