{
  "name": "story-inspector",
  "$schema": "../../../nx.json",
  "projectType": "library",
  "targets": {
    "prep": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn prep",
            "forwardAllArgs": false
          }
        ],
        "parallel": false
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn check",
            "forwardAllArgs": false
          }
        ],
        "parallel": false
      }
    }
  }
}
